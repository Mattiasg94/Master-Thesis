% offset = [1;1];
% obs_w_offset = obs(1:2)+[cos(obs(3)); sin(obs(3))];
% 
% scatter(obs(1),obs(2));
% hold on
% scatter(obs_w_offset(1),obs_w_offset(2))
dt = 0.1;
u(1) = 1;
x(3)
A = [ 1, 0, -dt*u(1)*sin(xtilde(3));
      0, 1,  dt*u(1)*cos(x(3));
      0, 0,    1];

B = [ dt*cos(x(3)) 0;
      dt*sin(x(3)) 0;
      0     dt];





